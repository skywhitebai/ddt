<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sky.ddt.dao.custom.CustomFbaPackingListRemarkHisMapper">
    <resultMap id="listFbaPackingListRemarkHisMap"
               type="com.sky.ddt.dto.fbaPackingListRemarkHis.resp.ListFbaPackingListRemarkHisResp"
               extends="com.sky.ddt.dao.generate.FbaPackingListRemarkHisMapper.BaseResultMap">
        <result column="createUserRealName" property="createUserRealName"/>
    </resultMap>
    <select id="listFbaPackingListRemarkHis" parameterType="com.sky.ddt.dto.fbaPackingListRemarkHis.req.ListFbaPackingListRemarkHisReq" resultMap="listFbaPackingListRemarkHisMap">
        select
        *,(select real_name from user u where u.user_id=his.create_by) createUserRealName
        from fba_packing_list_remark_his his
        where fba_packing_list_id=#{fbaPackingListId}
        order by his.create_time desc
    </select>
</mapper>