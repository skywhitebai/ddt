<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sky.ddt.dao.custom.CustomStockRecordItemPrintRecordMapper">
    <resultMap id="listStockRecordItemPrintRecordMap"
               type="com.sky.ddt.dto.stockRecordItemPrintRecord.resp.ListStockRecordItemPrintRecordResp"
               extends="com.sky.ddt.dao.generate.StockRecordItemPrintRecordMapper.BaseResultMap">
        <result column="createRealName" property="createRealName"/>
    </resultMap>
    <select id="listStockRecordItemPrintRecord"
            resultMap="listStockRecordItemPrintRecordMap">
        SELECT
        a.*,
         (SELECT real_name from user u where u.user_id=a.create_by)  createRealName
        from stock_record_item_print_record a
        where a.stock_record_item_id=#{stockRecordItemId}
        ORDER BY create_time desc
    </select>
</mapper>